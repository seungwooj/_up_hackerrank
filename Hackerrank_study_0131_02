    "#### 8. Mini-Max Sum   <- 어떻게 풀엇는지 물어보기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 2 3 4 5\n",
      "10 14\n"
     ]
    }
   ],
   "source": [
    "arr = list(map(int, input().strip().split(' ')))\n",
    "\n",
    "max_sum, min_sum, sum_ttl, sum_other = 0, 0, 0, 0\n",
    "\n",
    "for num in arr:\n",
    "    sum_ttl += num\n",
    "    \n",
    "min_sum = sum_ttl\n",
    "\n",
    "for i in range(len(arr)):\n",
    "    sum_others = sum_ttl - arr[i]\n",
    "    \n",
    "    if sum_others < min_sum:\n",
    "        min_sum = sum_others\n",
    "        \n",
    "    if sum_others > max_sum:\n",
    "        max_sum = sum_others\n",
    "        \n",
    "print(min_sum, max_sum)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 2 3 4 5\n",
      "10 14\n"
     ]
    }
   ],
   "source": [
    "import sys\n",
    "\n",
    "arr = list(map(int, input().strip().split(' ')))\n",
    "max=-sys.maxsize-1\n",
    "min=sys.maxsize\n",
    "sum=0\n",
    "for x in arr:\n",
    "    sum+=x\n",
    "    if x>max:\n",
    "        max=x\n",
    "    if x<min:\n",
    "        min=x\n",
    "print (sum-max,sum-min)"
   ]
  },
