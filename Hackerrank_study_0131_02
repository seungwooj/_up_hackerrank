    "ar = list(map(int, input().strip().split(' ')))\n",
    "\n",
    "A = B = C = 0\n",
    "for i in ar:\n",
    "    if i > 0:\n",
    "        A+=1\n",
    "\n",
    "    elif i < 0:\n",
    "        B+=1\n",
    "\n",
    "    else:\n",
    "        C+=1\n",
    "\n",
    "print(A/n)\n",
    "print(B/n)\n",
    "print(C/n)"
